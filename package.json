{
  "name": "@rodbe/eslint-config",
  "version": "1.1.0",
  "description": "Shareable config for eslint to keep all projects consistent",
  "keywords": [
    "eslint",
    "front-end",
    "RBX",
    "typescript"
  ],
  "homepage": "https://github.com/rodbe-io/eslint-config",
  "bugs": {
    "url": "https://github.com/rodbe-io/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodbe-io/eslint-config.git"
  },
  "license": "MIT",
  "author": "KevinRodbe",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./js": {
      "import": "./dist/js.js",
      "types": "./dist/js.d.ts"
    },
    "./next": {
      "import": "./dist/next.js",
      "types": "./dist/next.d.ts"
    },
    "./nx": {
      "import": "./dist/nx.js",
      "types": "./dist/nx.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./ts": {
      "import": "./dist/ts.js",
      "types": "./dist/ts.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "pnpm run ts:check",
    "build": "rm -rf dist && pkgroll",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "eslint": "eslint",
    "eslint:fix": "eslint --fix",
    "prepare": "husky || true",
    "release": "semantic-release",
    "test:unit": "echo 'no needed'",
    "ts:check": "tsc --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@commitlint/format": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "^9.21.0",
    "@eslint/json": "^0.10.0",
    "@rodbe/prettier-config": "^1.3.1",
    "@rodbe/tsconfig": "^1.0.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "10.0.6",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/node": "^22.13.5",
    "commitizen": "^4.3.1",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-package-json": "^0.26.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "pkgroll": "^2.11.0",
    "prettier": "^3.5.2",
    "prettier-plugin-curly": "^0.3.1",
    "prettier-plugin-packagejson": "^2.5.8",
    "semantic-release": "20.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "peerDependencies": {
    "eslint": ">=9"
  },
  "packageManager": "pnpm@9.15.2",
  "engines": {
    "node": ">= 18.18.2",
    "npm": ">= 8.11.0",
    "pnpm": ">= 9.15.2"
  },
  "volta": {
    "node": "18.18.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "enginesStrict": true
}
